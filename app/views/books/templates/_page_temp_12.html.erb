<div class="container" style="width: 200.1%; margin: 0;">
  <div class="row">
    <% @num = 1 %>
    <% if @book.bookpages[@id].images[@num-1].blank? %>
        <div id="<%= @book.bookpages[@id].id %>, <%= @num %>" class="bgmove col-xs-9 div_2" style="width: 75%; padding-top: 49.75%; margin: 0 0 0 0;">
          <%= render 'books/templates/rotate' %>
        </div>
    <% else %>
        <div id="<%= @book.bookpages[@id].id %>, <%= @num %>" class="bgmove col-xs-9 div_2" style="width: 75%; padding-top: 49.75%; margin: 0 0 0 0; background: url('<%= @book.bookpages[@id].images[@num-1].url %>') <%= @book.bookpages[@id].positions[@num-1] %> / cover  no-repeat;">
          <%= render 'books/templates/rotate' %>
        </div>
    <% end %>
    <div class="col-xs-3">
      <div class="row">
        <% @num = 2 %>
        <% if @book.bookpages[@id].images[@num-1].blank? %>
            <div id="<%= @book.bookpages[@id].id %>, <%= @num %>" class="bgmove col-xs-12 div_2" style="width: 90%; padding-top: 62%; margin: 4.25% 5% 1% 5%;">
              <%= render 'books/templates/rotate' %>
            </div>
        <% else %>
            <div id="<%= @book.bookpages[@id].id %>, <%= @num %>" class="bgmove col-xs-12 div_2" style="width: 90%; padding-top: 62%; margin: 4.25% 5% 1% 5%; background: url('<%= @book.bookpages[@id].images[@num-1].url %>') <%= @book.bookpages[@id].positions[@num-1] %> / cover  no-repeat;">
              <%= render 'books/templates/rotate' %>
            </div>
        <% end %>
      </div>
      <div class="row">
        <% @num = 3 %>
        <% if @book.bookpages[@id].images[@num-1].blank? %>
            <div id="<%= @book.bookpages[@id].id %>, <%= @num %>" class="bgmove col-xs-12 div_2" style="width: 90%; padding-top: 62%; margin: 1% 5% 1% 5%;">
              <%= render 'books/templates/rotate' %>
            </div>
        <% else %>
            <div id="<%= @book.bookpages[@id].id %>, <%= @num %>" class="bgmove col-xs-12 div_2" style="width: 90%; padding-top: 62%; margin: 1% 5% 1% 5%; background: url('<%= @book.bookpages[@id].images[@num-1].url %>') <%= @book.bookpages[@id].positions[@num-1] %> / cover  no-repeat;">
              <%= render 'books/templates/rotate' %>
            </div>
        <% end %>
      </div>
      <div class="row">
        <% @num = 4 %>
        <% if @book.bookpages[@id].images[@num-1].blank? %>
            <div id="<%= @book.bookpages[@id].id %>, <%= @num %>" class="bgmove col-xs-12 div_2" style="width: 90%; padding-top: 62%; margin: 1% 5% 4.25% 5%;">
              <%= render 'books/templates/rotate' %>
            </div>
        <% else %>
            <div id="<%= @book.bookpages[@id].id %>, <%= @num %>" class="bgmove col-xs-12 div_2" style="width: 90%; padding-top: 62%; margin: 1% 5% 4.25% 5%; background: url('<%= @book.bookpages[@id].images[@num-1].url %>') <%= @book.bookpages[@id].positions[@num-1] %> / cover  no-repeat;">
              <%= render 'books/templates/rotate' %>
            </div>
        <% end %>
      </div>
    </div>
  </div>
</div>