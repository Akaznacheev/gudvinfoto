<div class="container" style="width: 200%; margin: 0;">
  <div class="row">
    <div class="" style="width: 24%; display: inline-block;">
      <div class="row">
        <%= render partial: "books/templates/frame", locals: {bookpage: bookpage, num: 0, addclass: "col-xs-12", style: "width: 90%; padding-top: 62%; margin: 5% 5% 1% 5%;"} %>
      </div>
      <div class="row">
        <%= render partial: "books/templates/frame", locals: {bookpage: bookpage, num: 1, addclass: "col-xs-12", style: "width: 90%; padding-top: 62%; margin: 1% 5% 1% 5%;"} %>
      </div>
      <div class="row">
        <%= render partial: "books/templates/frame", locals: {bookpage: bookpage, num: 2, addclass: "col-xs-12", style: "width: 90%; padding-top: 62%; margin: 1% 5% 5% 5%;"} %>
      </div>
    </div>
    <%= render partial: "books/templates/frame", locals: {bookpage: bookpage, num: 3, addclass: "col-xs-9", style: "width: 75%; padding-top: 50%; margin: 0 0 0 0;"} %>
  </div>
</div>