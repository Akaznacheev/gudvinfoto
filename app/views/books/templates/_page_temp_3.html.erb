<% if @book.bookpages[@id].images.blank? %>
    <div id="<%= @book.bookpages[@id].id %>, 1" class="div_2" style="width: 60%; height: 60%; margin: 20%;">
    </div>
<% else %>
    <div id="<%= @book.bookpages[@id].id %>, 1" class="div_2" style="width: 60%; height: 60%; margin: 20%; background: url('<%= @book.bookpages[@id].images[0].url %>') no-repeat center; background-size: cover;">
    </div>
<% end %>