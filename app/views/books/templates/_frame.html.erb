<% if bookpage.images.blank? %>
    <div id="<%= bookpage.id %>, <%= num %>" class="bgmove <%= addclass %> div_2" style="<%= style %>" >
      <%= render partial: "books/templates/rotate", locals: {bookpage: bookpage, num: num} %>
    </div>
<% else %>
    <div id="<%= bookpage.id %>, <%= num %>" class="bgmove <%= addclass %> div_2" style="<%= style %> background: url('<%= bookpage.images[num] %>') <%= bookpage.positions[num] %> / cover  no-repeat;">
      <%= render partial: "books/templates/rotate", locals: {bookpage: bookpage, num: num} %>
    </div>
<% end %>