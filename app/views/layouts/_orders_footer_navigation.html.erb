<div class="orders_footer">
  <div class="column_left links">
    <ul>
      <%= render 'layouts/footer_navigation_links' %>
    </ul>
  </div>
  <div class="column_right icons">
    <ul>
      <% SocialIcon.where("length(icon_link) > 0").each do |icon|%>
        <li>
          <%= link_to 'http://' + icon.icon_link do %>
            <%= image_tag 'social_icons/' + icon.name + '.png', alt: '' %>
          <% end %>
        </li>
      <% end %>
    </ul>
  </div>
</div>