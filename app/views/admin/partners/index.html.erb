<div class="page_box">
  <div class="admin_content">
    <%= render 'admin/navigate_tabs' %>
    <h2>ПАРТНЕРЫ:</h2>
    <% @partners.each do |partner| %>
        <%= render partner %>
    <% end %>
    <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>"><%= value %></div>
    <% end %>
    <div style="width: 100%;float: left; padding-top: 16px; text-align: center;">
      <%= link_to 'ДОБАВИТЬ ПАРТНЁРА', new_admin_partner_path, class: 'round_button' %>
    </div>
  </div>
</div>